pipeline {
  agent any
    stages{
      stage('SCM Checkout'){
        steps{
	  git 'https://github.com/danikgithub/ansible-tomcat8-install.git'
        }
      }
      stage('Execute Ansible') {
        steps{
	  ansiblePlaybook become: true, becomeUser: 'jenkins', credentialsId: '383ba359-531e-458d-8528-9d3a37594371', installation: 'ansible2', inventory: 'hosts', playbook: 'ansible-tomcat8-install'
        }
      }
    }
  }
